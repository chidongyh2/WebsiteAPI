    <meta name="keywords" content="@(string.IsNullOrEmpty(ViewBag.Keyword) ? "Công ty cổ phần bệnh viện thái thịnh" : ViewBag.Keyword)">
    <meta name="description" content="@(string.IsNullOrEmpty(ViewBag.Description) ? Html.Raw("Với phương châm &#34;Luôn ân cần – Luôn cố gắng&#34;, trải qua 20 năm hoạt động, Phòng khám là địa chỉ uy tín trong lĩnh vực chăm sóc sức khỏe mà người dân Hà Nội và các tỉnh lân cận &#34;chọn mặt gửi vàng&#34;.") : Html.Raw(ViewBag.Description))" />
    <meta property="url" content="@Html.Raw(Request.Url)" />
    <meta property="type" content="website" />
    <meta property="title" content="@Html.Raw(ViewBag.Title)" />
    <meta property="description" content="@(string.IsNullOrEmpty(ViewBag.Description) ? Html.Raw("Với phương châm &#34;Luôn ân cần – Luôn cố gắng&#34;, trải qua 20 năm hoạt động, Phòng khám là địa chỉ uy tín trong lĩnh vực chăm sóc sức khỏe mà người dân Hà Nội và các tỉnh lân cận &#34;chọn mặt gửi vàng&#34;.") : Html.Raw(ViewBag.Description))" />
    <meta property="image" content="@(string.IsNullOrEmpty(ViewBag.Image) ? "/assets/images/logo-full-temp.png" : ViewBag.Image)" />
    <meta property="og:url" content="@Html.Raw(Request.Url)" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="@Html.Raw(ViewBag.Title)" />
    <meta property="og:description" content="@(string.IsNullOrEmpty(ViewBag.Description) ? Html.Raw("Với phương châm &#34;Luôn ân cần – Luôn cố gắng&#34;, trải qua 20 năm hoạt động, Phòng khám là địa chỉ uy tín trong lĩnh vực chăm sóc sức khỏe mà người dân Hà Nội và các tỉnh lân cận &#34;chọn mặt gửi vàng&#34;.") : Html.Raw(ViewBag.Description))" />
    <meta property="og:image" content="@(string.IsNullOrEmpty(ViewBag.Image) ? "/assets/images/logo-full-temp.png" : ViewBag.Image)" />
    @Styles.Render("~/Content/css")
</head>
