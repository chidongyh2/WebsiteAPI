    <title>@(string.IsNullOrEmpty(ViewBag.Title) ? "Phòng khám 125 Thái Thịnh - 20 năm phát triển" : ViewBag.Title)</title>

    <link rel="apple-touch-icon" sizes="76x76" href="/favicons/apple-touch-icon.png">
